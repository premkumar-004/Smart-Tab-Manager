{
    "manifest_version": 3,
    "name": "TabLinkedList",
    "version": "1.0",
    "description": "Converts all tabs into a single, linked-list style tab and manages memory.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}